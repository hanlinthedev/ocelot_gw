{
	"Routes": [
		{
			"UpstreamHttpMethod": ["Get", "Post"],
			"UpstreamPathTemplate": "/product/{everything}",
			"DownstreamPathTemplate": "/{everything}",
			"DownstreamScheme": "http",
			"ServiceName": "product",
			"DelegationHandlers": ["CustomHeaderDelegatingHandler"],
			"AuthenticationOptions": {
				"AuthenticationProviderKey": "ApiKeyScheme"
			},

			"RouteClaimsRequirement": {
				"ClientType": "User"
			},
			"LoadBalancerOptions": {
				"Type": "RoundRobin"
			}
		},
		{
			"UpstreamHttpMethod": ["Get", "Post"],
			"UpstreamPathTemplate": "/protected/{everything}",
			"DownstreamPathTemplate": "/{everything}",
			"DownstreamScheme": "http",
			"ServiceName": "protected",
			"DelegationHandlers": ["CustomHeaderDelegatingHandler"],
			"AuthenticationOptions": {
				"AuthenticationProviderKey": "ApiKeyScheme"
			},
			"RouteClaimsRequirement": {
				"ClientType": "Admin"
			},
			"LoadBalancerOptions": {
				"Type": "RoundRobin"
			}
		}
	],
	"GlobalConfiguration": {
		"ServiceDiscoveryProvider": {
			"Host": "consul",
			"Port": 8500,
			"ConfigurationKey": "services",
			"Type": "PollConsul",
			"PollingInterval": 100000
		}
	}
}
